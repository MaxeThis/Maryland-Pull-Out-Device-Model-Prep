{
    "build": {
        "beforeDevCommand": "",
        "beforeBuildCommand": "",
        "devPath": "../dist",
        "distDir": "../dist",
        "withGlobalTauri": true
    },
    "package": {
        "productName": "Maryland Pull-Out Device Model Prep",
        "version": "1.0.0"
    },
    "tauri": {
        "allowlist": {
            "all": false,
            "shell": {
                "all": false,
                "open": true
            },
            "window": {
                "all": true
            },
            "fs": {
                "all": true,
                "scope": [
                    "$HOME/**/*",
                    "$DESKTOP/**/*",
                    "$DOWNLOADS/**/*",
                    "$DOCUMENTS/**/*"
                ]
            },
            "dialog": {
                "all": true
            },
            "path": {
                "all": true
            }
        },
        "bundle": {
            "active": true,
            "targets": "all",
            "identifier": "com.maryland.pull-out-device",
            "icon": [
                "icons/icon.png",
                "icons/icon.icns",
                "icons/icon.ico"
            ],
            "resources": [
                "../dist/public/**/*"
            ],
            "externalBin": [],
            "copyright": "",
            "category": "Education",
            "shortDescription": "",
            "longDescription": "",
            "windows": {
                "certificateThumbprint": null,
                "digestAlgorithm": "sha256"
            },
            "macOS": {
                "signingIdentity": "Developer ID Application: Max Mendelson (TQ9NS86B63)",
                "minimumSystemVersion": "10.15"
            }
        },
        "security": {
            "csp": null
        },
        "windows": [
            {
                "fullscreen": false,
                "resizable": true,
                "title": "Maryland Pull-Out Device Model Prep",
                "width": 1400,
                "height": 900,
                "minWidth": 1000,
                "minHeight": 700,
                "decorations": false,
                "fileDropEnabled": false
            }
        ]
    }
}